separator=false

[disk]
interval=30
separator=false
command=echo -n " ";~/.config/i3blocks/blocklets/disk

[iface]
interval=10
command=echo -n " ";~/.config/i3blocks/blocklets/iface --interface

[cpu-usage]
interval=persist
command=go run ~/.config/i3blocks/blocklets/cpu_usage.go
markup=pango

[memory]
interval=10
command=echo -n " ";~/.config/i3blocks/blocklets/memory

[temperature]
interval=10
command=echo -n " ";go run ~/.config/i3blocks/blocklets/temperature.go

[pulsevolume]
interval=once
signal=1
command=echo -n " ";~/.config/i3blocks/blocklets/pulsevolume

[time]
interval=1
command=date '+%Y-%m-%d %H:%M:%S'
